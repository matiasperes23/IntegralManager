        <Button Name="buttonCerrar" Background="#FFCCCCCC" BorderThickness="0" Content="Cancelar" FontFamily="Gisha" FontWeight="Bold" Foreground="#FF222222" Height="25" Margin="0,0,12,12" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="90" Click="buttonCerrar_Click" />
        <Label Content="Seleccione las disciplinas en las que desea dar de alta/baja a Nombre Socio " Height="28" HorizontalAlignment="Left" Margin="9,3,0,0" Name="labelAltaBaja" VerticalAlignment="Top" />
    </Grid>
</Window>
