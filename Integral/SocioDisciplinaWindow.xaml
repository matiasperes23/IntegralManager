        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dar de baja a: " Height="210" Width="550" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit">
    <Grid>
        <xctk:CheckListBox DisplayMemberPath="Nombre" Margin="10,34,12,43" Name="listBoxDisciplinas" Padding="5" ValueMemberPath="" />
