<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <connectionStrings>
    <add name="integralDataEntities" connectionString="metadata=res://*/IntegralModel.csdl|res://*/IntegralModel.ssdl|res://*/IntegralModel.msl;provider=System.Data.SQLite;provider connection string=&quot;data source=|DataDirectory|\integralData.db&quot;" providerName="System.Data.EntityClient"/>
